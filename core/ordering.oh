#ordering: #one_of
[    equal:
     left:
     right:
]

is_bigger(~t_0:, t_1:): #ordering
{    if t_0 > t_1
          ~# left
     elif t_1 > t_0
          ~# right
     else
          ~# equal
}

is_smaller(~t_0:, t_1:): #ordering
{    if t_0 < t_1
          ~# left
     elif t_1 < t_0
          ~# right
     else
          ~# equal
}
